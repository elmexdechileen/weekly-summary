{
  "compilerOptions": {
    "target": "ES6",
    "module": "ES6",
    "strict": true,
    "moduleResolution": "node",
    "esModuleInterop": true,
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true,
    "outDir": "./"
  },
  "include": ["./main.ts"],
  "exclude": ["node_modules"]
}
